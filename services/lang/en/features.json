            "uiFeatures": [
              {
                "feature": "SpanText",
                "children": "Check your junk folder. If it still has not arrived, you can "
              },
              {
                "feature": "LinkText",
                "children": "ask us to send you another email",
                "props": {
                  "href": "/password-recover/_restart/",
