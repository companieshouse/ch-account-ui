  "RESET_PASSWORD_3": [
    {
      "feature": "PageHeading",
      "children": "Check your phone"
    },
    {
      "feature": "BodyText",
      "props": {
        "uiFeatures": [
          {
            "feature": "SpanText",
            "children": "We've sent you a text message with a security code to "
          },
          {
            "feature": "SpanText",
            "props": {
              "weight": "bold",
              "uiFeatures": [
                {
                  "feature": "TemplateText",
                  "props": {
                    "template": "${phoneNumber}"
                  }
                }
              ]
