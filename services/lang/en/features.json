              {
                "feature": "LinkText",
                "children": "give us a different email address",
                "props": {
                  "href": "/password-recover/_restart/",
                  "testId": "restartPasswordRecoveryLink"
                }
              },
              {
                "feature": "SpanText",
                "children": "."
              }
            ]
          }
